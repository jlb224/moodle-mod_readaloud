<div class="mod_readaloud_attemptsummary">
    <div class="mod_readaloud_center">
        <h5>{{#str}}myattemptssummary, mod_readaloud, {{summary.totalattempts}} {{/str}}</h5>
    </div>
    <div class="mod_readaloud_center">
        {{#str}}summaryexplainer, mod_readaloud{{/str}}
    </div>
    <div class="mod_readaloud_attemptsummarytable table-responsive">
        <table id="uniqid" class ="generaltable mod_readaloud_table">
            <thead>
                <tr>
                    <th class="header c0"></th>
                    <th class="header c1">{{#str}}wpm, mod_readaloud{{/str}}</th>
                    <th class="header c2">{{#str}}accuracy_p, mod_readaloud{{/str}}</th>
                    {{#showgrades}}
                        <th class="header c3">{{#str}}grade_p, mod_readaloud{{/str}}</th>
                    {{/showgrades}}
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td class="cell c0">{{#str}}averages, mod_readaloud{{/str}}
                    <td class="cell c1">{{summary.av_wpm}}</td>
                    <td class="cell c2">{{summary.av_accuracy}}</td>
                    {{#showgrades}}
                        <td class="cell c3">{{summary.av_sessionscore}}</td>
                    {{/showgrades}}
                </tr>
                <tr>
                    <td class="cell c0">{{#str}}highest, mod_readaloud{{/str}}
                    <td class="cell c1">{{summary.h_wpm}}</td>
                    <td class="cell c2">{{summary.h_accuracy}}</td>
                    {{#showgrades}}
                        <td class="cell c3">{{summary.h_sessionscore}}</td>
                    {{/showgrades}}
                </tr>
            </tbody>
        </table>
    </div>
<div>